
fn main() i32 {
  SIZE := 128u;
  data : [SIZE]u8 = {};
  nbytes := readf(&data[0], SIZE);
  printf("got %d bytes: ", nbytes);
  print_cstr(&data[0]);
  printf("\n");
  printf("strlen result: %d\n", strlen(&data[0]));

  str := string.new(&data[0]);
  str2 := string.new("hello: ");
  printf("size of string: %d\nprint_str: ", sizeof(str));
  print_str(str);
  printf("\n");
  printf("combined string: ");
  print_str(str2 + str);
  printf("\n");
  ret 0;
}
