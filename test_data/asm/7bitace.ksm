; the first ever demo written for memepu: 7bit ace. 2019/06/29
dw 0 ; no interrupts
lh r5,f000 ; test sgn ext.
lhu r0,ffff
addu r0,r0,1 ; r0 is vram addr
addu r2,r0,0 ; set r2 to r0
lhu r1,0 ; r1 is color
loop:
  sw [r0,0],r1
  addu r0,r0,29
  addu r1,r1,7
  or r0,r0,r2  ; make sure vram bit is still set.
  lhu r31,loop
